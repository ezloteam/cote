    "charm": "1.0.2",
    "colors": "1.3.3",
    "core-js": "^2.6.8",
    "eventemitter2": "5.0.1",
    "lodash": "^4.17.11",
    "mqtt": "^2.18.8",
    "portfinder": "1.0.20",
    "socket.io": "2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "async": "^2.6.2",
    "ava": "^1.4.1",
    "commander": "2.20.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "humanize-number": "0.0.2",
    "nyc": "^14.1.1",
    "randomstring": "^1.1.5",
    "request": "^2.88.0",
    "sinon": "^7.3.2",
    "socket.io-client": "2.2.0"
