  "keywords": [
    "microservices",
    "zero-configuration",
    "zero-conf",
    "mesh",
    "network",
    "auto discovery",
    "scalable",
    "fault-tolerant"
  ],
  "description": "A Node.js library for building zero-configuration microservices.",
